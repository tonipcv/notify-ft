generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model DeviceToken {
  id           String   @id @default(uuid())
  deviceToken  String   @unique
  userId       String   @default("an√¥nimo")
  platform     String   @default("ios")
  registeredAt DateTime @default(now())
} 